{
  "name": "nourriture-solidaire-dashboard",
  "version": "1.0.0",
  "description": "Dashboard Nourriture Solidaire avec Tailwind CSS",
  "scripts": {
    "build:css": "npx tailwindcss -i ./css/input.css -o ./css/tailwind.min.css --minify",
    "watch:css": "npx tailwindcss -i ./css/input.css -o ./css/tailwind.min.css --watch",
    "dev": "npx tailwindcss -i ./css/input.css -o ./css/tailwind.min.css --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  }
}
